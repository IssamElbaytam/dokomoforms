-- Table: survey

-- DROP TABLE survey;

CREATE TABLE survey
(
  survey_id    uuid PRIMARY KEY DEFAULT uuid_generate_v4(),

  title        text                     NOT NULL,
  survey_owner text                     NOT NULL,
  created_on   timestamp with time zone NOT NULL default now(),

  CONSTRAINT survey_title_survey_owner_key UNIQUE (title, survey_owner)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE survey
  OWNER TO postgres;

